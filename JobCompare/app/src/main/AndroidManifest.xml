<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.seclass.jobcompare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdateJob"></activity>
        <activity android:name=".EnterUpdateJob" />
        <activity android:name=".TwoJobs" />
        <activity android:name=".CompareJobs" />
        <activity android:name=".JobOffersDBSQLiteHelper" />
        <activity android:name=".JobOffersDB" />
        <activity android:name=".ComparisonSettings" />
        <activity android:name=".CurrentJobDetails" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>